    "name": "tautcony.github.io"
  },
  "scripts": {
    "submodule": "git submodule init && git submodule update",
    "build:dev": "npx webpack --config build/webpack.dev.conf.js",
    "build:prod": "npx webpack --config build/webpack.prod.conf.js",
    "build:update": "npx webpack --config build/webpack.update.conf.js",
    "start": "bundle exec jekyll server -I -H 0.0.0.0",
    "start:l": "bundle exec jekyll server -I -l -H 0.0.0.0",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 version",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "IE_Mob 11"
  ],
